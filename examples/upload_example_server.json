{
  "tool": "upload_mcp_server",
  "parameters": {
    "name": "Simple Python MCP Server",
    "description": "A basic example Python MCP server for demonstration purposes",
    "archive_base64": "",
    "command": [
      "python",
      "simple_python_server.py"
    ],
    "environment": {
      "PYTHONPATH": ".",
      "MCP_SERVER_NAME": "simple-example"
    },
    "auto_restart": true,
    "resource_limits": {
      "memory_mb": 256,
      "cpu_percent": 30
    },
    "user_id": "example_creator"
  }
}